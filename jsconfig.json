{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./node_modules",
    "paths": {
      "@components/*": ["../src/components/*"],
      "@pages/*": ["../src/pages/*"],
      "@images/*": ["../src/assets/images/*"],
      "@scss/*": ["../src/assets/scss/*"],
      "@/*": ["../src/*"]
    }
  },
  "exclude": ["build"]
}